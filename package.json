{
  "name": "slim",
  "version": "0.5.1",
  "homepage": "https://github.com/herrmannlab/slim",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "lint": "ts-standard --env jest 'src/**/*.{tsx,ts}'",
    "fmt": "ts-standard --env jest 'src/**/*.{tsx,ts}' --fix",
    "test": "ts-standard --env jest 'src/**/*.{tsx,ts}' && craco test --setupFiles ./src/setupTests.tsx --watchAll=false",
    "predeploy": "REACT_APP_CONFIG=demo PUBLIC_URL='https://herrmannlab.github.io/slim/' craco build",
    "deploy": "gh-pages -d build",
    "clean": "rm -rf ./build ./node_modules"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@craco/craco": "^6.4.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/retry": "^0.12.1",
    "@types/uuid": "^8.3.0",
    "antd": "^4.20.5",
    "classnames": "^2.2.6",
    "craco-less": "^2.0.0",
    "dcmjs": "^0.22.0",
    "detect-browser": "^5.2.1",
    "dicom-microscopy-viewer": "^0.37.1",
    "dicomweb-client": "^0.8.4",
    "gh-pages": "^4.0.0",
    "oidc-client": "^1.11.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.1.0",
    "retry": "^0.13.1",
    "ts-standard": "^11.0.0",
    "typescript": "^4.6.4"
  }
}
